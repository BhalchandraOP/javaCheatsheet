package introduction_ObjectOrientedProgramming;

public class MotorBikeRunner {
	public static void main(String[] args) {
		MotorBike ducati = new MotorBike();
		MotorBike Honda = new MotorBike();
		
// state of objects
		
		ducati.start();
		Honda.start();
		
		
	ducati.speed=100;
	honda.speed=80;
	}

}
